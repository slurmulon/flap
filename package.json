{
  "name": "flap",
  "version": "0.1.9",
  "description": "Guard clauses for ES6",
  "main": "./lib/flap.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "lint": "eslint src/flap.js",
    "start": "babel-node src/flap.js",
    "pretest": "npm run build",
    "test": "mocha --reporter nyan --compilers js:babel/register test",
    "auto-start": "nodemon --exec \"npm run lint && npm start\" --ignore public/js",
    "preinstall": "npm install babel@5.8.21",
    "postinstall": "npm run build",
    "prepublish": "npm run build",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --compilers js:babel/register"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/slurmulon/flap.git"
  },
  "keywords": [
    "flap",
    "condition",
    "state",
    "complex",
    "arguments",
    "args",
    "query",
    "all",
    "any",
    "when",
    "unless",
    "clause",
    "guard"
  ],
  "author": "MadHax",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/slurmulon/flap/issues"
  },
  "dependencies": {
    "jsonpath": "^0.1.4"
  },
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-eslint": "^4.0.10",
    "blanket": "^1.2.1",
    "chai": "^3.2.0",
    "chai-as-promised": "^5.1.0",
    "chai-things": "^0.2.0",
    "eslint": "^1.1.0",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.2",
    "nodemon": "^1.4.1"
  }
}
