{
  "name": "flap",
  "version": "0.1.11",
  "description": "Guard clauses for ES6",
  "main": "./lib/flap.js",
  "scripts": {
    "build": "node ./node_modules/.bin/babel src --out-dir lib",
    "lint": "node ./node_modules/babel-eslint/index.js src/flap.js",
    "start": "node ./node_modules/.bin/babel-node src/flap.js",
    "pretest": "npm run build",
    "test": "mocha --reporter nyan --compilers js:babel-register test",
    "auto-start": "nodemon --exec \"npm run lint && npm start\" --ignore public/js",
    "postinstall": "npm run build",
    "prepublish": "npm run build",
    "coverage": "node ./node_modules/.bin/nyc --require babel-core/register node_modules/.bin/mocha test/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/slurmulon/flap.git"
  },
  "keywords": [
    "flap",
    "condition",
    "state",
    "complex",
    "arguments",
    "args",
    "query",
    "all",
    "any",
    "when",
    "unless",
    "clause",
    "guard"
  ],
  "author": "MadHax",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/slurmulon/flap/issues"
  },
  "dependencies": {
    "json-rel": "~0.0.8"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.3",
    "babel-plugin-syntax-flow": "^6.5.0",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "blanket": "^1.2.1",
    "chai": "^3.2.0",
    "chai-as-promised": "^5.1.0",
    "chai-things": "^0.2.0",
    "eslint": "^2.8.0",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.2",
    "nodemon": "^1.4.1",
    "nyc": "^6.4.0"
  }
}
